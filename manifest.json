{
  "name": "Tabs Tabs Tabs",
  "description": "Puts tabs in idle so you can have all the tabs open",
  "version": "0.1.3",
  "manifest_version": 3,
  "homepage_url": "https://github.com/ChromeAdmin/tabs-tabs-tabs-browser-extension",
  "icons": {
    "16": "icons/black-icon-128.png",
    "48": "icons/black-icon-128.png",
    "128": "icons/black-icon-128.png"
  },
  "action": {
    "default_title": "Tabs Tabs Tabs",
    "default_icon": "icons/black-icon-128.png"
  },
  "permissions": [
    "tabs",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "src/background.js"
  }
}